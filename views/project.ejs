<div class="container">
    <center><h3>Add a new Project</h3></center>
    <form action="/new/project" method="POST">
        <div class="mb-3">
            <label for="project_name" class="form-label">Project Name</label>
            <input type="text" name="project_name" class="form-control" id="project_name" placeholder="Facebook clone">
        </div>
        <div class="mb-3">
            <label for="project_link" class="form-label">Project Link</label>
            <input type="text" class="form-control" name="project_link" id="project_link" placeholder="www.facebook.com">
        </div>
        <div class="mb-3">
            <label for="project_description" class="form-label">Example textarea</label>
            <textarea class="form-control" name="project_description" id="project_description" rows="2"></textarea>
        </div>
        <div class="container text-center">
            <div class="row">
                <div class="col">
                    <div class="mb-3">
                        <label for="project_name" class="form-label">Project Link</label>
                        <input type="text" name="project_link1" class="form-control" id="project_name" placeholder="https:www.facebook.com">
                    </div>
                </div>
                <div class="col">
                    OR
                </div>
                <div class="col">
                    <div class="mb-3">
                        <label for="formFile" class="form-label">File Upload</label>
                        <input class="form-control" name="project_link2" type="file" id="formFile">
                    </div>
                </div>
            </div>
        </div>
        <label for="exampleDataList" class="form-label">Project Category</label>
        <input class="form-control" name="project_category" list="datalistOptions" id="exampleDataList" placeholder="Type to search...">
        <datalist id="datalistOptions">
            <option value="Front End">
            <option value="Back End">
            <option value="Full Stack">
            <option value="others">
        </datalist>
        <div class="mb-3">
            <label for="Github Link" class="form-label">Github Link</label>
            <input type="text" class="form-control" name="project_repo" id="github_link" placeholder="www.github.com/facebook.git">
        </div>

    <center><button class="btn btn-primary btn-lg">Submit Project Details</button></center>
    </form>

</div>